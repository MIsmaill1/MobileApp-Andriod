<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activity.ComplaintWithDrawSuccess">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:orientation="vertical">
            <include
                layout="@layout/top_city_logo_with_border">
            </include>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_100sdp"
            android:orientation="vertical">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center"
                android:layout_marginBottom="@dimen/_50sdp"
                android:textColor="@color/black"
                android:layout_marginStart="40dp"
                android:layout_marginEnd="40dp"
                android:textSize="@dimen/_14sdp"
                android:visibility="gone"
                android:fontFamily="@font/sweetsanspro_regular"
                android:text="@string/chose_the_category_under_which_your_complaint_falls">
            </TextView>

            <com.airbnb.lottie.LottieAnimationView
                android:id="@+id/success_Shared_property"
                android:layout_width="140dp"
                android:layout_height="140dp"
                android:layout_marginTop="@dimen/_50sdp"
                android:layout_marginBottom="@dimen/_30sdp"
                android:layout_gravity="center"
                app:lottie_autoPlay="true"
                app:lottie_rawRes="@raw/lf30_editor_5sxuciel"
                app:lottie_loop="false"
                app:lottie_speed="1.25" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center"
                android:layout_marginStart="40dp"
                android:layout_marginEnd="40dp"
                android:textSize="@dimen/_12sdp"
                android:fontFamily="@font/sweetsanspro_regular"
                android:text="@string/your_complaint_has_been_withdraw">
            </TextView>

        </LinearLayout>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>